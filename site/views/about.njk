{% extends 'layout.njk' %}

{% block head %}  
<title>Sobre</title>
{% endblock %}

{% block content %}
    
    <div id="wrapper" style>
        <img src="{{data.avatar_url}}" alt="Ruth Maria">
        <h1> {{ data.name }} </h1>
        <h2> {{ data.role1 }} </h2>
        <h2> {{ data.role2 }} </h2>
        <h2> {{ data.role3 }} </h2>
            
        <p> {{ data.description }} </p>
    </div>
    
       
    <div id="links-footer" class="links">
        {% for link in data.links %} 
            <a href="{{ link.url }} " target="_blank"> {{ link.name }} </a>
        {% endfor %}
     </div> 
    
 {% endblock %}